# Currency
VITE_NATIVE_CURRENCY=BNB
VITE_NATIVE_CURRENCY_SYMBOL=BNB
VITE_NATIVE_CURRENCY_DECIMALS=18

# =========================================
# API Configuration
# =========================================
# Development
VITE_API_URL=http://localhost:5000/api

# Production
# VITE_API_URL=https://api.yourdomain.com/api

# =========================================
# Default Fee Configuration (in percentage)
# =========================================
# Management fee (annual)
VITE_DEFAULT_MANAGEMENT_FEE=2

# Performance fee (on profits)
VITE_DEFAULT_PERFORMANCE_FEE=20

# Entry fee (on deposits)
VITE_DEFAULT_ENTRY_FEE=1

# Exit fee (on withdrawals)
VITE_DEFAULT_EXIT_FEE=1

# =========================================
# Default Portfolio Configuration
# =========================================
# Initial investment amount in BNB
VITE_DEFAULT_INITIAL_AMOUNT=0.1

# Minimum holding amount in BNB
VITE_DEFAULT_MIN_HOLDING=0.01

# =========================================
# BNB Chain Specific Configuration
# =========================================
# Gas Price Configuration (in Gwei)
VITE_DEFAULT_GAS_PRICE=5
VITE_MAX_GAS_PRICE=20

# Block Explorer URLs
VITE_BLOCK_EXPLORER_URL=https://bscscan.com
# VITE_BLOCK_EXPLORER_URL=https://testnet.bscscan.com

# =========================================
# Feature Flags
# =========================================
# Enable/disable features
VITE_ENABLE_PORTFOLIO_TRANSFER=true
VITE_ENABLE_PUBLIC_PORTFOLIOS=true
VITE_ENABLE_TOKEN_WHITELIST=true

# =========================================
# UI Configuration
# =========================================
# Theme configuration
VITE_DEFAULT_THEME=dark

# Display configuration
VITE_DEFAULT_CURRENCY=BNB
VITE_DEFAULT_LANGUAGE=en

# =========================================
# Analytics and Monitoring
# =========================================
# Enable/disable analytics
VITE_ENABLE_ANALYTICS=false

# Analytics API key (if enabled)
VITE_ANALYTICS_API_KEY=your-analytics-api-key

# =========================================
# Security Configuration
# =========================================
# Maximum number of failed login attempts
VITE_MAX_LOGIN_ATTEMPTS=5

# Session timeout in minutes
VITE_SESSION_TIMEOUT=30

# =========================================
# Development Configuration
# =========================================
# Enable/disable development features
VITE_ENABLE_DEV_TOOLS=true

# Development API endpoints
VITE_DEV_API_URL=http://localhost:5000/api
VITE_DEV_WS_URL=ws://localhost:5000

# =========================================
# Production Configuration
# =========================================
# Production API endpoints
# VITE_PROD_API_URL=https://api.yourdomain.com/api
# VITE_PROD_WS_URL=wss://api.yourdomain.com

# =========================================
# Cache Configuration
# =========================================
# Cache duration in seconds
VITE_CACHE_DURATION=300

# Enable/disable cache
VITE_ENABLE_CACHE=true

# =========================================
# Error Handling
# =========================================
# Enable/disable error reporting
VITE_ENABLE_ERROR_REPORTING=true

# Error reporting API key
VITE_ERROR_REPORTING_API_KEY=your-error-reporting-api-key

# =========================================
# Rate Limiting
# =========================================
# API rate limit (requests per minute)
VITE_API_RATE_LIMIT=60

# =========================================
# Logging Configuration
# =========================================
# Log level (debug, info, warn, error)
VITE_LOG_LEVEL=info

# Enable/disable logging
VITE_ENABLE_LOGGING=true